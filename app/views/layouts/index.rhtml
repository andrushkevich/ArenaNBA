<script type="text/javascript">
function detectBrowser()
{
var IE6 = (navigator.userAgent.indexOf("MSIE 6")>=0) ? true : false;
if ( IE6 )
{
alert("Sorry, our site doesn't work with your browser. Please try to use another one. FireFox, IE7, Opera , Safari for example.");
}
}
</script>
